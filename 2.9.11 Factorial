public class Factorial extends ConsoleProgram
{
    public void run()
    {
        //takes number from user
        int userNum = readInt("What number would you like to factor? ");
        
        //loop to factor number
        for(int i = userNum-1; i > 0; i--)
        {
            userNum *= i;
        }
        
        //prints factored result
        System.out.println(userNum);
    }
}
